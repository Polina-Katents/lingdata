    <TIER ANNOTATOR="Polina-Katents" DEFAULT_LOCALE="en"
        LINGUISTIC_TYPE_REF="translation" PARENT_REF="text@nsi1931f"
        PARTICIPANT="@nsi1931f" TIER_ID="trans@nsi1931f">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a33" ANNOTATION_REF="a1">
                <ANNOTATION_VALUE>Well, and they all came like "Gurilla, guerilla",</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a35" ANNOTATION_REF="a2">
                <ANNOTATION_VALUE>went to our house right away, climbled up to the ceiling, ransacked everything but didn’t find anything an- anywhere. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a36" ANNOTATION_REF="a3">
                <ANNOTATION_VALUE>Nobody.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a37" ANNOTATION_REF="a4">
                <ANNOTATION_VALUE>Well who could, like, dad just,</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a38" ANNOTATION_REF="a5">
                <ANNOTATION_VALUE> erm, </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a39" ANNOTATION_REF="a6">
                <ANNOTATION_VALUE>left like that and, hmm, didn't come back.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a40" ANNOTATION_REF="a7">
                <ANNOTATION_VALUE>Well...</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a41" ANNOTATION_REF="a8">
                <ANNOTATION_VALUE>Didn't know anything. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a42" ANNOTATION_REF="a9">
                <ANNOTATION_VALUE>Until the war ended</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a43" ANNOTATION_REF="a10">
                <ANNOTATION_VALUE>Didn't hear anything, and when the war ended then we got the, ugh, the death notification.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a44" ANNOTATION_REF="a11">
                <ANNOTATION_VALUE>For him, that he died in Stalingrad in '42</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a45" ANNOTATION_REF="a12">
                <ANNOTATION_VALUE>in April.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a46" ANNOTATION_REF="a13">
                <ANNOTATION_VALUE>Yes, well.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a47" ANNOTATION_REF="a14">
                <ANNOTATION_VALUE> Mom was called up to the dist- to the dis- ugh, district committee.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a48" ANNOTATION_REF="a15">
                <ANNOTATION_VALUE>Called her up to the district committee and she went and got... this...death notification.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a49" ANNOTATION_REF="a16">
                <ANNOTATION_VALUE>Well, that's true.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a50" ANNOTATION_REF="a18">
                <ANNOTATION_VALUE>We were, we were.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a51" ANNOTATION_REF="a19">
                <ANNOTATION_VALUE>No one wor…warned us, and the Germans were already retreating, and they’ve already filled our whole village.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a52" ANNOTATION_REF="a20">
                <ANNOTATION_VALUE>There were n- none of our people lef-,  and we were so scared, so we left for the forest.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
    </TIER>
    <TIER ANNOTATOR="Polina-Katents" DEFAULT_LOCALE="en"
        LINGUISTIC_TYPE_REF="translation" PARENT_REF="text@zb1950f"
        PARTICIPANT="@zb1950" TIER_ID="trans@zb1950">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a53" ANNOTATION_REF="a21">
                <ANNOTATION_VALUE>That summer the Germans came to the village.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a54" ANNOTATION_REF="a22">
                <ANNOTATION_VALUE>You, y-... did you... know anything about him, like, where he was?</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a55" ANNOTATION_REF="a23">
                <ANNOTATION_VALUE>Didn't hear anything!</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a56" ANNOTATION_REF="a24">
                <ANNOTATION_VALUE>M, yeah.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a57" ANNOTATION_REF="a34">
                <ANNOTATION_VALUE>Yes.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a58" ANNOTATION_REF="a25">
                <ANNOTATION_VALUE> Yeah.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a59" ANNOTATION_REF="a26">
                <ANNOTATION_VALUE>In '42</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a60" ANNOTATION_REF="a27">
                <ANNOTATION_VALUE>Well, it was like that.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a61" ANNOTATION_REF="a28">
                <ANNOTATION_VALUE>At least there was something.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a62" ANNOTATION_REF="a29">
                <ANNOTATION_VALUE>Well, probably it left just, like, the deepest impression, could you remind us, like, well, how the village was freed.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a63" ANNOTATION_REF="a30">
                <ANNOTATION_VALUE>The Russians were approaching, the Germans were running off,  so, how did you hear, er, you, mom said that they, ugh, that you were somewhere in that Dlinniy brook.</ANNOTATION_VALUE>
